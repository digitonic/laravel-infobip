{
    "name": "digitonic/laravel-infobip",
    "description": "Accessing infobip API via HTTP facade",
    "type": "laravel-package",
    "autoload": {
        "psr-4": {
            "Digitonic\\Infobip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digitonic\\Infobip\\": "src/",
            "Digitonic\\Infobip\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitonic\\Infobip\\Providers\\InfobipServiceProvider"
            ],
            "aliases": {
                "Infobip": "Digitonic\\Infobip\\Facades\\Infobip"
            }
        }
    },
    "authors": [
        {
            "name": "falko189",
            "email": "daniele@digitonic.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "^9.41",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.14",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "laravel/pint": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
